        <segment begin="0x0000" end="0x007F" reference="ascii" type="ascii" />
        <segment begin="0x0080" end="0x0080" reference="buffer" offset="0" type="euro-sign" />
        <segment begin="0x0081" end="0x813F" reference="0xFFFD" type="undefine" />
        <segment begin="0x8140" end="0xFFFF" reference="buffer" offset="1" type="gbk-multiple-char" />
    </charmap>
