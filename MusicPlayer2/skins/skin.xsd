          </xs:enumeration>
          <xs:enumeration value="find">
            <xs:annotation>
              <xs:documentation>查找</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="abRepeat">
            <xs:annotation>
              <xs:documentation>AB重复</xs:documentation>
