              <xs:documentation>菜单</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="miniMode">
            <xs:annotation>
              <xs:documentation>迷你模式</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="miniModeClose">
