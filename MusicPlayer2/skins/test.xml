<root xmlns="https://github.com/zhongyang219/MusicPlayer2/skin" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://github.com/zhongyang219/MusicPlayer2/skin ./skin.xsd"
      name="test">
  
  <!--歌曲详情面板-->
  <panel id="song_info_panel" width="450" height="270">
    <verticalLayout>
      <!--标题栏-->
      <rectangle height="24">
        <horizontalLayout>
          <text type="userDefine" text="%(UI_TIP_BTN_PROPERTY)" style="scroll" margin-left="4"/>
          <button key="closePanel" width="24"/>
        </horizontalLayout>
      </rectangle>
      <!--导航栏-->
      <navigationBar margin="4" margin-bottom="0" height="28" item_space="4" stack_element_id="song_info_panel_stack_element">
        <navigationItem icon="albumCover" text="封面/详情"/>
        <navigationItem icon="reverb" text="%(UI_TXT_SPECTRUM)"/>
      </navigationBar>
      <stackElement id="song_info_panel_stack_element" margin="4">
        <!--专辑封面/曲目详情-->
        <horizontalLayout margin="4">
          <albumCover square="true" max-width="206"/>
          <verticalLayout margin-left="8">
            <trackInfo height="24"/>
            <horizontalLayout height="24">
              <icon icon="lyric" width="24"/>
              <text type="userDefine" width="70" text="%(TXT_TITLE):" color_style="emphasis1"/>
              <text type="userDefine" text="%(Title)" style="scroll"/>
            </horizontalLayout>
            <horizontalLayout height="24">
              <icon icon="artist" width="24"/>
              <text type="userDefine"  width="70" text="%(TXT_ARTIST):" color_style="emphasis1"/>
              <text type="userDefine" text="%(Artist)" style="scroll"/>
            </horizontalLayout>
            <horizontalLayout height="24">
              <icon icon="album" width="24"/>
              <text type="userDefine"  width="70" text="%(TXT_ALBUM):" color_style="emphasis1"/>
              <text type="userDefine" text="%(Album)" style="scroll"/>
            </horizontalLayout>
            <horizontalLayout height="24">
              <icon icon="fileRelate" width="24"/>
              <text type="userDefine"  width="70" text="%(TXT_FILE_TYPE):" color_style="emphasis1"/>
              <text type="userDefine" text="%(FileType)" style="scroll"/>
            </horizontalLayout>
            <horizontalLayout height="24">
              <icon icon="bitrate" width="24"/>
              <text type="userDefine"  width="70" text="%(TXT_BITRATE):" color_style="emphasis1"/>
              <text type="userDefine" text="%(BitRate)kbps" style="scroll"/>
            </horizontalLayout>
            <horizontalLayout height="24">
              <icon icon="reverb" width="24"/>
              <text type="userDefine"  width="70" text="%(TXT_PROPERTY_DLG_FILE_CHANNELS)" color_style="emphasis1"/>
              <text type="userDefine" text="%(Channels)" style="scroll"/>
            </horizontalLayout>
            <rating height="24" margin-left="4"/>
          </verticalLayout>
        </horizontalLayout>
        <!--频谱分析-->
        <verticalLayout margin="4">
          <text type="title" alignment="center" font_size="12" style="scroll" height="32"/>
          <text type="artist" alignment="center" font_size="10" style="scroll" height="32"/>
          <text type="album" alignment="center" font_size="10" style="scroll" height="32"/>
          <spectrum alignment="center" max-height="30%" fixed_width="true" draw_reflex="true" margin-top="24" proportion="4"/>
        </verticalLayout>
      </stackElement>

    </verticalLayout>
  </panel>
  
  <!--播放队列面板-->
  <panel id="play_queue_panel" width="300" height="350">
    <verticalLayout>
      <rectangle height="24">
        <horizontalLayout>
          <text type="userDefine" text="%(UI_TXT_PLAY_QUEUE)" margin-left="4"/>
          <button key="closePanel" width="24"/>
        </horizontalLayout>
      </rectangle>
      <verticalLayout margin="4">
        <text type="userDefine" text="%(TXT_LIB_STATISTICS_TOTAL_NUM_OF_TRACK): %(PlayQueueNum)" height="24"/>
        <searchBox height="26" margin-bottom="4"/>
        <playlist item_height="32"/>
      </verticalLayout>
    </verticalLayout>
  </panel>
  
  <!--最近播放面板-->
  <panel id="renect_played_panel" width="300" height="350">
    <verticalLayout>
      <rectangle height="24">
        <horizontalLayout>
          <text type="userDefine" text="%(TXT_RECENT_PLAYED)" margin-left="4"/>
          <button key="closePanel" width="24"/>
        </horizontalLayout>
      </rectangle>
      <verticalLayout margin="4">
        <searchBox height="26" margin-bottom="4"/>
        <recentPlayedList item_height="32"/>
      </verticalLayout>
    </verticalLayout>
  </panel>

  <!--左侧导航面板-->
  <panel id="navigation_panel" width="180" margin-left="0">
    <verticalLayout margin="4">
      <horizontalLayout height="36" margin-bottom="4">
        <button key="closePanel" icon="menu" text="%(UI_TXT_CLOSE_NAVIGATION)" width="36"/>
        <placeHolder/>
      </horizontalLayout>
      <navigationBar orientation="vertical" icon_type="icon_and_text" item_space="4" item_left_space="2" item_height="36" stack_element_id="main_stack_element">
        <navigationItem icon="playlistDisplayMode" text="水平标签"/>
        <navigationItem icon="playlistDock" text="垂直标签"/>
        <navigationItem icon="keyBoard" text="文本测试"/>
        <navigationItem icon="reverb" text="频谱分析/按钮"/>
        <navigationItem icon="star" text="按钮/图标测试"/>
        <navigationItem icon="folderExplore" text="文件夹浏览"/>
        <navigationItem icon="lyric" text="歌词/播放列表"/>
      </navigationBar>
    </verticalLayout>
  </panel>

  <ui>
    <verticalLayout>
      <horizontalLayout>
        <!--左侧导航栏-->
        <rectangle width="44" no_corner_radius="true">
          <verticalLayout margin="4">
            <button key="showPanel" icon="menu" text="%(UI_TXT_OPEN_NAVIGATION)" panel_id="navigation_panel" width="36" height="36" margin-bottom="4"/>
            <navigationBar width="36" orientation="vertical" icon_type="icon_only" item_space="4" item_height="36" stack_element_id="main_stack_element">
              <navigationItem icon="playlistDisplayMode" text="水平标签"/>
              <navigationItem icon="playlistDock" text="垂直标签"/>
              <navigationItem icon="keyBoard" text="文本测试"/>
              <navigationItem icon="reverb" text="频谱分析/按钮"/>
              <navigationItem icon="star" text="按钮/图标测试"/>
              <navigationItem icon="folderExplore" text="文件夹浏览"/>
              <navigationItem icon="lyric" text="歌词/播放列表"/>
            </navigationBar>
          </verticalLayout>
        </rectangle>
        <stackElement id="main_stack_element" margin-top="4" show_indicator="false">
          <!--第1页 水平标签-->
          <verticalLayout>
            <!--导航栏-->
            <navigationBar height="30" icon_type="icon_only" item_space="4" stack_element_id="stackElement1">
              <!--使用navigationItem元素指定导航栏按钮-->
              <navigationItem icon="nowPlaying" text="%(UI_TXT_PLAYSTATUS_PLAYING)"/>
              <navigationItem icon="reverb" text="%(UI_TXT_SPECTRUM)"/>
              <navigationItem icon="lyric" text="%(UI_TXT_LYRICS)"/>
              <navigationItem icon="playInPlaylist" text="%(UI_TXT_PLAY_QUEUE)"/>
              <navigationItem icon="history" text="%(TXT_RECENT_PLAYED)"/>
              <navigationItem icon="folder" text="%(TXT_FOLDER)"/>
              <navigationItem icon="playlist" text="%(TXT_PLAYLIST)"/>
              <navigationItem icon="favoriteOn" text="%(TXT_MY_FAVOURITE)"/>
              <navigationItem icon="artist" text="%(TXT_ARTIST)"/>
              <navigationItem icon="album" text="%(TXT_ALBUM)"/>
              <navigationItem icon="genre" text="%(TXT_GENRE)"/>
              <navigationItem icon="year" text="%(TXT_YEAR)"/>
              <navigationItem icon="fileRelate" text="%(TXT_FILE_TYPE)"/>
              <navigationItem icon="bitrate" text="%(TXT_BITRATE)"/>
              <navigationItem icon="star" text="%(TXT_RATING)"/>
              <navigationItem icon="folderExplore" text="%(TXT_FOLDER_EXPLORE)"/>
              <navigationItem icon="mediaLib" text="%(TXT_ALL_TRACKS)"/>
            </navigationBar>
            <stackElement id="stackElement1" margin="8">
              <!--正在播放-->
              <verticalLayout>
                <albumCover square="true" margin="28" margin-bottom="12"/>
                <text type="title" style="scroll" height="36" font_size="12" margin-left="16" margin-right="16" alignment="center"/>
                <text type="artist_album" style="scroll" height="24" font_size="10" margin-left="16" margin-right="16" alignment="center"/>
              </verticalLayout>
              <!--频谱分析-->
              <verticalLayout>
                <text type="title" alignment="center" font_size="12" style="scroll" height="32"/>
                <text type="artist" alignment="center" font_size="10" style="scroll" height="32"/>
                <text type="album" alignment="center" font_size="10" style="scroll" height="32"/>
                <spectrum alignment="center" max-height="30%" fixed_width="true" draw_reflex="true" margin-top="24" proportion="4"/>
              </verticalLayout>
              <!--歌词-->
              <lyrics/>
              <!--播放队列-->
              <verticalLayout>
                <playlistIndicator height="30" margin-bottom="4"/>
                <searchBox height="26" margin-bottom="4"/>
                <playlist/>
              </verticalLayout>
              <!--最近播放-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <recentPlayedList track_list_element_id="recent_played_track_list"/>
                </verticalLayout>
                <trackList id="recent_played_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--文件夹-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <mediaLibFolder track_list_element_id="folder_track_list"/>
                </verticalLayout>
                <trackList id="folder_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--播放列表-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <mediaLibPlaylist track_list_element_id="playlist_track_list"/>
                </verticalLayout>
                <trackList id="playlist_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--我喜欢的音乐-->
              <verticalLayout max-width="520">
                <horizontalLayout height="26" margin-bottom="4">
                  <button key="playMyFavourite" height="28" show_text="true" proportion="10"/>
                  <placeHolder proportion="1"/>
                  <searchBox max-width="200" proportion="10"/>
                </horizontalLayout>
                <myFavouriteList/>
              </verticalLayout>
              <!--艺术家-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <mediaLibItemList type="artist" track_list_element_id="artist_track_list"/>
                </verticalLayout>
                <trackList id="artist_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--唱片集-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <mediaLibItemList type="album" track_list_element_id="album_track_list"/>
                </verticalLayout>
                <trackList id="album_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--流派-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <mediaLibItemList type="genre" track_list_element_id="genre_track_list"/>
                </verticalLayout>
                <trackList id="genre_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--年份-->
              <mediaLibItemList type="year"/>
              <!--文件类型-->
              <mediaLibItemList type="file_type"/>
              <!--文件类型-->
              <!--比特率-->
              <mediaLibItemList type="bitrate"/>
              <!--分级-->
              <mediaLibItemList type="rating"/>
              <!--文件夹浏览-->
              <horizontalLayout>
                <verticalLayout max-width="520">
                  <searchBox height="26" margin-bottom="4"/>
                  <medialibFolderExplore track_list_element_id="folder_explore_track_list"/>
                </verticalLayout>
                <trackList id="folder_explore_track_list" margin-left="8" hide-width="520"/>
              </horizontalLayout>
              <!--所有曲目-->
              <verticalLayout>
                <searchBox height="26" margin-bottom="4"/>
                <allTracksList/>
              </verticalLayout>
            </stackElement>
          </verticalLayout>

          <!--第2页 垂直标签-->
          <horizontalLayout>
            <!--导航栏。使用item_list属性和navigationItem子节点都可以设置导航的标签按钮。item_list属性指定的标签会显示在前面-->
            <navigationBar width="120" margin="4" orientation="vertical" icon_type="icon_and_text" item_space="4" item_height="32" font_size="9"
                        item_list="now_playing,spectrum,lyrics,play_queue,recently_played,folder,playlist,my_favourite,artist,album,genre,year,file_type,bitrate,rating">
              <navigationItem icon="folderExplore" text="%(TXT_FOLDER_EXPLORE)"/>
              <navigationItem icon="mediaLib" text="%(TXT_ALL_TRACKS)"/>
            </navigationBar>
            <stackElement margin="8" scroll_to_switch="true">
              <verticalLayout>
                <albumCover square="true" margin="28" margin-bottom="12"/>
                <text type="title" style="scroll" height="36" font_size="12" margin-left="16" margin-right="16" alignment="center"/>
                <text type="artist_album" style="scroll" height="24" font_size="10" margin-left="16" margin-right="16" alignment="center"/>
              </verticalLayout>
              <verticalLayout>
                <text type="title" alignment="center" font_size="12" style="scroll" height="32"/>
                <text type="artist" alignment="center" font_size="10" style="scroll" height="32"/>
                <text type="album" alignment="center" font_size="10" style="scroll" height="32"/>
                <spectrum alignment="center" max-height="30%" fixed_width="true" draw_reflex="true" margin-top="24" proportion="4"/>
              </verticalLayout>
              <lyrics/>
              <verticalLayout>
                <playlistIndicator height="36" margin-bottom="8" font_size="11"/>
                <playlist font_size="10" item_height="34"/>
              </verticalLayout>
              <recentPlayedList/>
              <mediaLibFolder/>
              <mediaLibPlaylist/>
              <myFavouriteList/>
              <mediaLibItemList type="artist"/>
              <mediaLibItemList type="album"/>
              <mediaLibItemList type="genre"/>
              <mediaLibItemList type="year"/>
              <mediaLibItemList type="file_type"/>
              <mediaLibItemList type="bitrate"/>
              <mediaLibItemList type="rating"/>
              <medialibFolderExplore/>
              <verticalLayout>
                <searchBox height="26" margin-bottom="4"/>
                <allTracksList/>
              </verticalLayout>
            </stackElement>
          </horizontalLayout>

          <!--第3页 文本测试-->
          <verticalLayout margin="4" margin-top="0">
            <navigationBar height="30" icon_type="icon_and_text" item_space="4">
              <navigationItem icon="tag" text="变量测试"/>
              <navigationItem icon="keyBoard" text="文本样式测试"/>
            </navigationBar>
            <stackElement margin-top="4">
              <!--变量测试-->
              <horizontalLayout>
                <!--第1列-->
                <verticalLayout margin-right="4">
                  <horizontalLayout height="20">
                    <text text="文件名:" width="100" color_style="emphasis1"/>
                    <text text="%(FileName)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="文件路径:" width="100" color_style="emphasis1"/>
                    <text text="%(FilePath)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="标题:" width="100" color_style="emphasis1"/>
                    <text text="%(Title)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="艺术家:" width="100" color_style="emphasis1"/>
                    <text text="%(Artist)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="唱片集:" width="100" color_style="emphasis1"/>
                    <text text="%(Album)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="唱片集艺术家:" width="100" color_style="emphasis1"/>
                    <text text="%(AlbumArtist)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="艺术家-唱片集:" width="100" color_style="emphasis1"/>
                    <text text="%(ArtistAndAlbum)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="音轨号:" width="100" color_style="emphasis1"/>
                    <text text="%(TrackNum)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="年份:" width="100" color_style="emphasis1"/>
                    <text text="%(Year)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="流派:" width="100" color_style="emphasis1"/>
                    <text text="%(Genre)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="注释:" width="100" color_style="emphasis1"/>
                    <text text="%(Comment)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="文件类型:" width="100" color_style="emphasis1"/>
                    <text text="%(FileType)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="比特率:" width="100" color_style="emphasis1"/>
                    <text text="%(BitRate)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="声道数:" width="100" color_style="emphasis1"/>
                    <text text="%(Channels)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="采样频率:" width="100" color_style="emphasis1"/>
                    <text text="%(SampleFreq)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="位深度:" width="100" color_style="emphasis1"/>
                    <text text="%(BitDepth)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="歌词文件:" width="100" color_style="emphasis1"/>
                    <text text="%(LyricsFile)" style="scroll"/>
                  </horizontalLayout>
                </verticalLayout>
                <!--第2列-->
                <verticalLayout margin-left="4">
                  <horizontalLayout height="20">
                    <text text="格式:" width="100" color_style="emphasis1"/>
                    <text text="%(Format)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="Midi音色库名称:" width="100" color_style="emphasis1"/>
                    <text text="%(MidiSoundFont)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="Midi音乐的速度:" width="100" color_style="emphasis1"/>
                    <text text="%(MidiSpeed)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="Midi音乐的位置:" width="100" color_style="emphasis1"/>
                    <text text="%(MidiPosition)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="Midi音乐的长度:" width="100" color_style="emphasis1"/>
                    <text text="%(MidiLength)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="播放状态:" width="100" color_style="emphasis1"/>
                    <text text="%(PlayingStatus)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="播放项目类型:" width="100" color_style="emphasis1"/>
                    <text text="%(PlayModeName)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="播放项目名称:" width="100" color_style="emphasis1"/>
                    <text text="%(PlayListName)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="当前位置:" width="100" color_style="emphasis1"/>
                    <text text="%(CurrentPosition)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="长度:" width="100" color_style="emphasis1"/>
                    <text text="%(SongLength)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="歌词:" width="100" color_style="emphasis1"/>
                    <text text="%(CurrentLyric)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="循环模式:" width="100" color_style="emphasis1"/>
                    <text text="%(RepeatMode)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="音量:" width="100" color_style="emphasis1"/>
                    <text text="%(Volume)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="帧率:" width="100" color_style="emphasis1"/>
                    <text text="%(FPS)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="播放队列曲目数:" width="100" color_style="emphasis1"/>
                    <text text="%(PlayQueueNum)" style="scroll"/>
                  </horizontalLayout>
                  <horizontalLayout height="20">
                    <text text="播放曲目序号:" width="100" color_style="emphasis1"/>
                    <text text="%(PlayQueueIndex)" style="scroll"/>
                  </horizontalLayout>
                </verticalLayout>
              </horizontalLayout>
              <!--文本样式测试-->
              <verticalLayout>
                <trackInfo height="28"/>
                <trackInfo font_size="10" height="28"/>
                <albumCover proportion="2"/>
                <text type="title" font_size="16" style="scroll" height="32"/>
                <text type="artist" font_size="14" style="scroll" height="32" color_style="emphasis1"/>
                <text type="album" font_size="12" style="scroll" height="32" color_style="emphasis2"/>
                <!--<placeHolder height="24" hide-height="200"/>-->
                <rectangle proportion="1">
                  <lyrics margin="8" use_default_font="true" font_size="9"/>
                </rectangle>
                <text text="%(CurrentLyric)" height="24"/>
                <horizontalLayout height="24">
                  <text text="默认颜色" color_style="default"/>
                  <text text="强调色1" color_style="emphasis1"/>
                  <text text="强调色2" color_style="emphasis2"/>
                </horizontalLayout>
              </verticalLayout>
            </stackElement>
          </verticalLayout>

          <!--第4页 频谱分析/按钮-->
          <verticalLayout margin="4">
            <text height="24" text="频谱分析测试"/>
            <verticalLayout>
              <spectrum type="128col" margin="8" min-height="40"/>
              <spectrum type="64col" margin="8" min-height="40"/>
              <horizontalLayout min-height="40">
                <spectrum type="32col" margin="8" min-height="40" width="50%"/>
                <spectrum type="16col" margin="8" min-height="40" width="25%"/>
                <spectrum type="8col" margin="8" min-height="40" width="13%"/>
              </horizontalLayout>
            </verticalLayout>
            <verticalLayout>
              <text height="24" text="带标签的图标测试"/>
              <horizontalLayout height="32">
                <button key="menu" height="28" margin="2" show_text="true" font_size="10"/>
                <button key="miniMode" height="28" margin="2" show_text="true"/>
                <button key="fullScreen" height="28" margin="2" show_text="true"/>
                <!--<button key="repeatMode" height="28" margin="2" show_text="true"/>-->
                <button key="settings" height="28" margin="2" show_text="true"/>
                <button key="equalizer" height="28" margin="2" show_text="true"/>
                <button key="darkLightMode" height="28" margin="2" show_text="true"/>
                <button key="skin" height="28" margin="2" show_text="true"/>
                <button key="settings" height="28" width="28" margin="2"/>
                <button key="info" height="28" margin="2" show_text="true" text="详细信息按钮"/>
                <button key="info" height="28" width="28" margin="2" show_text="false" text="详细信息按钮" bigIcon="true"/>
                <button key="showPanel" icon="folderExplore" text="%(TXT_FOLDER_EXPLORE)" height="28" width="28" margin="2" panel_file_name="folder_explore_panel.xml"/>
                <button key="showPanel" icon="folder" text="%(TXT_FOLDER)" height="28" width="28" margin="2" panel_file_name="folder_explore_panel.xml"/>
              </horizontalLayout>
              <text height="24" text="工具栏测试"/>
              <rectangle height="24">
                <horizontalLayout>
                  <button key="repeatMode" width="20" height="20" margin="2"/>
                  <button key="settings" width="20" height="20" margin="2"/>
                  <button key="equalizer" width="20" height="20" margin="2"/>
                  <button key="skin" width="20" height="20" margin="2"/>
                  <button key="miniMode" width="20" height="20" margin="2" hide-width="174"/>
                  <button key="info" width="20" height="20" margin="2" hide-width="198"/>
